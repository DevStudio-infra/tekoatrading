    "winston": "^3.10.0",
    "@clerk/clerk-sdk-node": "^4.13.15",
    "cors": "^2.8.5",
    "langchain": "^0.1.30",
    "@google-cloud/aiplatform": "^3.18.0",
    "@langchain/google-genai": "^0.0.15",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2",
    "prisma": "^5.15.0",
    "ts-node": "^10.9.2",
    "@types/winston": "^2.4.4",
    "@types/cors": "^2.8.17"
